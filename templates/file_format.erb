                "paths": [ 
                            <% @paths.each do |path| -%>
                                <% if path != @paths.last -%>
                                    "<%= path %>",
                                <% else -%>
                                    "<%= path %>"
                                <% end -%>
                            <% end -%>
                         ],
                "fields": { 
                                <% @fields.keys.sort.each do |key| -%>
                                    "<%= key %>:"
                                        <% @fields[key].each do |val| -%>
                                            <% if @fields[key] != @fields.last -%>
                                                "<%= val %>",
                                            <% else %->
                                                "<%= val %>"
                                            <% end -%>
                                        <% end -%>
                                <% end -%>
                          }
            },
